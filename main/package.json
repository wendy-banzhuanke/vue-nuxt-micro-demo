{
  "name": "main",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn serve\" \"yarn serve:seamodule\" \"yarn serve:nuxtdemo\"",
    "install:seamodule": "cd ../seamodule && yarn install",
    "install:nuxtdemo": "cd ../nuxtdemo && yarn install",
    "install:all": "concurrently \"yarn install:seamodule\"  \"yarn install\"",
    "serve:seamodule": "cd ../seamodule && yarn serve",
    "serve:nuxtdemo": "cd ../nuxtdemo && yarn dev",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "qiankun": "^2.0.2",
    "vue": "^2.6.11",
    "@vue/composition-api": "^0.5.0",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "element-ui": "^2.12.0",
    "node-sass": "^4.13.0",
    "postcss-easings": "^2.0.0",
    "sass-loader": "^8.0.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-nuxt": ">=0.4.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.3.0",
    "lodash": "^4.17.21",
    "tailwindcss": "^1.0.5"
  },
  "resolutions": {
    "vue": "2.6.11",
    "vue-server-renderer": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "workspaces": [
    "externals/*"
  ]
}
